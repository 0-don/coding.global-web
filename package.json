{
  "name": "coding.global-web",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"dotenvx run -- vinxi dev\" \"typesafe-i18n\"",
    "build": "dotenvx run -- vinxi build",
    "start": "dotenvx run -- vinxi start",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --format stylish",
    "gen": "dotenvx run -- npm run drop-db & drizzle-kit drop && drizzle-kit push:pg && drizzle-kit generate:pg",
    "drop-db": "dotenvx run -- node ./db.migrations/reset-db.js",
    "drop": "dotenvx run -- drizzle-kit drop",
    "push": "dotenvx run -- drizzle-kit push:pg",
    "generate": "dotenvx run -- drizzle-kit generate:pg",
    "i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@auth/drizzle-adapter": "0.8.0",
    "@dotenvx/dotenvx": "^0.26.0",
    "@elysiajs/eden": "^1.0.5",
    "@kobalte/core": "^0.12.6",
    "@solid-mediakit/auth": "latest",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.1",
    "@solidjs/start": "^1.0.0-rc.0",
    "@tanstack/solid-query": "^5.28.6",
    "drizzle-orm": "^0.30.4",
    "drizzle-typebox": "^0.1.1",
    "postgres": "3.4.4",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.16",
    "typesafe-i18n": "^5.26.2",
    "vinxi": "^0.3.10"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@tanstack/solid-query-devtools": "^5.28.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.14",
    "elysia": "^1.0.7",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-drizzle": "0.2.3",
    "eslint-plugin-solid": "^0.13.2",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18"
  }
}