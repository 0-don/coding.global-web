{
  "name": "coding.global-web",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -- vinxi dev",
    "build": "dotenvx run -- vinxi build",
    "start": "dotenvx run -- vinxi start",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --format stylish",
    "gen": "dotenvx run -- npm run drop-db & dotenvx run -- drizzle-kit drop && dotenvx run -- drizzle-kit push && dotenvx run -- drizzle-kit generate",
    "drop-db": "dotenvx run -- node ./db.migrations/reset-db.js",
    "drop": "dotenvx run -- drizzle-kit drop",
    "push": "dotenvx run -- drizzle-kit push:pg",
    "generate": "dotenvx run -- drizzle-kit generate:pg"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@auth/drizzle-adapter": "1.4.2",
    "@dotenvx/dotenvx": "^1.11.1",
    "@elysiajs/eden": "^1.1.2",
    "@kobalte/core": "^0.13.5",
    "@solid-mediakit/auth": "latest",
    "@solid-primitives/i18n": "^2.1.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.3",
    "@solidjs/start": "^1.0.6",
    "@tanstack/solid-query": "^5.52.2",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.33.0",
    "drizzle-typebox": "^0.1.1",
    "elysia": "^1.1.8",
    "postgres": "3.4.4",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.21",
    "vinxi": "^0.4.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.52.0",
    "@tanstack/solid-query-devtools": "^5.52.2",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.24.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-drizzle": "0.2.3",
    "eslint-plugin-solid": "^0.14.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}